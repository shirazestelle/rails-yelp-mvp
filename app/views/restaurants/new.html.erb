<div class="container">
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :category %>
    <%= f.text_field :category %>

    <%= f.submit class: "btn btn-primary" %>
  <% end %>
</div>
